{
  "name": "superfast-ai",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "typecheck": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "vectorize:create": "wrangler vectorize create superfast-ai-index --dimensions=1024 --metric=cosine",
    "migrate:local": "wrangler d1 migrations apply superfast-ai --local",
    "migrate:remote": "wrangler d1 migrations apply superfast-ai"
  },
  "dependencies": {
    "@ai-sdk-tools/agents": "^0.2.2",
    "@ai-sdk/anthropic": "^2.0.31",
    "@ai-sdk/google": "^2.0.23",
    "@ai-sdk/openai": "^2.0.52",
    "@ai-sdk/perplexity": "^2.0.13",
    "@aws-sdk/client-s3": "^3.910.0",
    "@aws-sdk/s3-request-presigner": "^3.910.0",
    "@hono/zod-validator": "^0.7.4",
    "@libsql/client": "^0.15.15",
    "@mastra/ai-sdk": "^0.2.1",
    "@microlabs/otel-cf-workers": "^1.0.0-rc.52",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.206.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@types/lodash": "^4.17.20",
    "@upstash/vector": "^1.2.2",
    "ai": "^5.0.72",
    "async_hooks": "^1.0.0",
    "better-auth": "^1.3.27",
    "better-sqlite3": "^12.4.1",
    "braintrust": "^0.4.6",
    "drizzle-orm": "^0.44.6",
    "exa-js": "^1.10.2",
    "hono": "^4.9.12",
    "i": "^0.3.7",
    "lodash": "^4.17.21",
    "mastra": "^0.17.0",
    "nanoid": "^5.1.6",
    "neverthrow": "^8.2.0",
    "p-limit": "^7.1.1",
    "pdf-lib": "^1.17.1",
    "unpdf": "^1.3.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
